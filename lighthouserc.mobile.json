{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/servicos",
        "http://localhost:3000/precos",
        "http://localhost:3000/faq"
      ],
      "numberOfRuns": 3,
      "settings": {
        "throttlingMethod": "devtools",
        "formFactor": "mobile",
        "screenEmulation": { "mobile": true }
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.90 }]
      }
    }
  }
}
